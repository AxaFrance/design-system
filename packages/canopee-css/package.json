{
    "name": "@axa-fr/canopee-css",
    "version": "0.1.0",
    "description": "",
    "sass": "dist/slash.scss",
    "style": "dist/slash.css",
    "exports": {
        "./logo-axa.svg": "./files/assets/logo-axa.svg",
        "./dist/common/assets/logo-axa.svg": "./files/assets/logo-axa.svg",
        "./dist/prospect-client/*": "./dist/prospect-client/*",
        "./dist/distributeur/*": "./dist/distributeur/*",
        "./dist/*.css": "./dist/*.css"
    },
    "files": [
        "dist",
        "files"
    ],
    "scripts": {
        "prebuild": "rimraf dist && npm run copyfiles",
        "build": "postcss \"src/**/*{Apollo,LF,Grid,DebugGrid,Svg,All}.css\" \"src/common\"  \"src/*.css\" \"src/distributeur/**/*.css\" --base src --dir dist --verbose --env production",
        "dev": "npm run build -- --watch",
        "copyfiles": "copyfiles --up 1 \"src/**/*.scss\" files/**/*.svg dist",
        "stylelint": "stylelint \"src/**/*.{scss,css}\" --ignore-pattern \"src/common/tokens*.css\"",
        "stylelint:fix": "npm run stylelint -- --fix"
    },
    "dependencies": {
        "rc-slider": "^11.1.7"
    },
    "devDependencies": {
        "@axa-fr/postcss-config-design-system": "*",
        "@axa-fr/prettier-config-design-system": "*",
        "@axa-fr/stylelint-config-design-system": "*",
        "copyfiles": "^2.4.1",
        "rimraf": "^6.0.1"
    },
    "peerDependencies": {
        "@material-symbols/svg-400": ">= 0.19.0"
    },
    "peerDependenciesMeta": {
        "@material-symbols/svg-400": {
            "optional": true
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AxaFrance/design-system.git"
    },
    "author": {
        "name": "AxaFrance"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AxaFrance/design-system/issues"
    },
    "homepage": "https://github.com/AxaFrance/design-system#readme",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.(scss|css)": "stylelint --fix"
    },
    "prettier": "@axa-fr/prettier-config-design-system",
    "stylelint": {
        "extends": "@axa-fr/stylelint-config-design-system"
    },
    "volta": {
        "extends": "../../package.json"
    }
}