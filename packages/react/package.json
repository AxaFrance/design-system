{
  "name": "@axa-fr/design-system-react",
  "version": "0.1.0",
  "description": "",
  "exports": {
    "./agent": {
      "import": "./dist/agent.js",
      "types": "./dist/agent.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
