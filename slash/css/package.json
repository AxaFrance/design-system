{
  "name": "@axa-fr/design-system-slash-css",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "postcss \"src/**/*.scss\" --base src --dir dist --ext css --verbose --watch --env development_agent --config ../../config/postcss.config.js",
    "build": "postcss \"src/**/*.scss\" --base src --dir dist --ext css --verbose --env production_agent --config ../../config/postcss.config.js",
    "start:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "build-storybook": "storybook build",
    "eslint": "eslint src --ext js,jsx,ts,tsx",
    "eslint:fix": "eslint src --ext js,jsx,ts,tsx --fix",
    "stylelint": "stylelint \"src/**/*.{scss,css}\"",
    "stylelint:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "prettier": "prettier \"src/**/*\" --check",
    "prettier:fix": "prettier \"src/**/*\" --write",
    "tsc": "tsc --noEmit",
    "chromatic": "npx chromatic"
  },
  "dependencies": {
    "rc-slider": "^11.1.7"
  },
  "peerDependencies": {
    "@material-symbols/svg-400": ">= 0.19.0"
  },
  "peerDependenciesMeta": {
    "@material-symbols/svg-400": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AxaFrance/design-system.git"
  },
  "author": {
    "name": "AxaFrance"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AxaFrance/design-system/issues"
  },
  "homepage": "https://github.com/AxaFrance/design-system#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint --fix",
    "*.(ts|tsx)": "tsc-files --noEmit",
    "*.(scss|css)": "stylelint --fix",
    "*.mdx": "prettier --write"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
