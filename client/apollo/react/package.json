{
  "name": "@axa-fr/design-system-apollo-react",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "import": "./src/index.ts"
      },
      "default": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint src --ext js,jsx,ts,tsx --fix",
    "prettier": "prettier \"src/**/*.!(js|jsx|ts|tsx|svg)\" --check",
    "prettier:fix": "prettier \"src/**/*.!(js|jsx|ts|tsx|svg)\" --write",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AxaFrance/design-system.git"
  },
  "author": {
    "name": "AxaFrance"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AxaFrance/design-system/issues"
  },
  "homepage": "https://github.com/AxaFrance/design-system#readme",
  "peerDependencies": {
    "@material-symbols/svg-400": ">= 0.19.0",
    "react": ">= 18"
  },
  "peerDependenciesMeta": {
    "@material-symbols/svg-400": {
      "optional": true
    }
  },
  "dependencies": {
    "@axa-fr/design-system-apollo-css": "*",
    "@fontsource/source-sans-pro": "^5.0.8",
    "@tanem/svg-injector": "^10.1.68",
    "classnames": "^2.5.1",
    "dompurify": "^3.1.5",
    "rc-slider": "^11.1.7"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint --fix",
    "*.(ts|tsx)": "tsc-files --noEmit",
    "*.mdx": "prettier --write"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}
